<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="200.0" maxWidth="220.0" minHeight="200.0" minWidth="220.0" prefHeight="200.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.LoginWindowControlled">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label layoutX="95.0" layoutY="31.0" text="Login" />
      <Label layoutX="85.0" layoutY="73.0" text="Password" />
      <TextField fx:id="loginField" layoutX="35.0" layoutY="48.0" promptText="Enter your login" />
      <PasswordField fx:id="passwordField" layoutX="35.0" layoutY="89.0" promptText="Enter your password" />
      <Button defaultButton="true" layoutX="35.0" layoutY="128.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="25.0" prefWidth="68.0" text="Log In" />
      <Button fx:id="signupButton" layoutX="116.0" layoutY="128.0" mnemonicParsing="false" onAction="#signupButtonClicked" prefHeight="25.0" prefWidth="68.0" text="Sign Up" />
   </children>
</AnchorPane>
